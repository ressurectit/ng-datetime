{
    "name": "@anglr/datetime",
    "version": "0.0.0-placeholder",
    "description": "Angular library for datetime manipulation components",
    "type": "module",
    "sideEffects": false,
    "scripts": {
        "build": "ngc --declaration --declarationDir . --declarationMap && ngc -t es2015 --outDir es2015 && npm run dts:rollup",
        "dts:rollup": "npm run api -- api-extractor.json && npm run api -- date-fns/api-extractor.json && npm run api -- avsc/api-extractor.json && npm run api -- stompjs/api-extractor.json && npm run api -- moment/api-extractor.json",
        "api": "api-extractor run --local --verbose -c",
        "clean": "rimraf */temp temp es2015 es2020 **/tsdoc-metadata.json src/**/*.d.ts src/**/*.d.ts.map */src/**/*.d.ts */src/**/*.d.ts.map ./*.d.ts ./*.d.ts.map */*.d.ts",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "version:placeholder": "replace-in-file /\\\"version\\\":\\s\\\".*?\\\"/g \"\\\"version\\\": \\\"0.0.0-placeholder\\\"\" package.json --isRegex",
        "version:git": "ngv -e \"replace-in-file 0.0.0-placeholder $GIT_VERSION package.json && echo $GIT_VERSION > version.bak\"",
        "version:build": "ngv -e \"echo $GIT_VERSION > version.bak\"",
        "prepublishOnly": "npm run bundle && npm run version:placeholder && npm run version:git",
        "postpublish": "npm run doc && npm run version:placeholder && npm run clean",
        "bundle": "npm run clean && npm run build",
        "deploy": "npm publish --loglevel error",
        "deploy:next": "npm publish --tag next --loglevel error",
        "test": "karma start karma.conf.ts --single-run",
        "lint": "eslint src/**/*.ts",
        "doc": "path-exists docs && npm run doc:generate || echo \"No docs generated!\"",
        "predoc:generate": "cd docs && git pull && cd ..",
        "postdoc:generate": "npm run doc:save",
        "doc:generate": "npm run doc:generate:datetime && npm run doc:generate:date-fns && npm run doc:generate:moment",
        "doc:save": "cd docs && git add . && git commit -m \"INT: updated API docs ng-datetime\" && git push && cd ..",
        "doc:generate:rest": "rimraf docs/content/api/ng-rest && npm run doc:json:rest && npm run doc:md:rest && npm run doc:api:rest",
        "doc:json:rest": "api-extractor run --local --verbose -c api-extractor.json",
        "doc:md:rest": "api-documenter markdown -i temp -o docs/content/api/ng-rest",
        "doc:api:rest": "copyfiles -u 1 temp/rest.api.md docs/content/api/ng-rest && rimraf temp",
        "doc:generate:moment": "rimraf docs/content/api/ng-rest-moment && npm run doc:json:moment && npm run doc:md:moment && npm run doc:api:moment",
        "doc:json:moment": "api-extractor run --local --verbose -c moment/api-extractor.json",
        "doc:md:moment": "api-documenter markdown -i moment/temp -o docs/content/api/ng-rest-moment",
        "doc:api:moment": "copyfiles -u 2 moment/temp/rest-moment.api.md docs/content/api/ng-rest-moment && rimraf moment/temp",
        "doc:generate:date-fns": "rimraf docs/content/api/ng-rest-date-fns && npm run doc:json:date-fns && npm run doc:md:date-fns && npm run doc:api:date-fns",
        "doc:json:date-fns": "api-extractor run --local --verbose -c date-fns/api-extractor.json",
        "doc:md:date-fns": "api-documenter markdown -i date-fns/temp -o docs/content/api/ng-rest-date-fns",
        "doc:api:date-fns": "copyfiles -u 2 date-fns/temp/common-date-fns.api.md docs/content/api/ng-common-date-fns && rimraf date-fns/temp"
    },
    "engines": {
        "node": "^12.20.0 || ^14.15.0 || >=16.10.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ressurectit/ng-datetime"
    },
    "keywords": [
        "angular",
        "ng",
        "datetime",
        "date"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ressurectit/ng-datetime/issues"
    },
    "homepage": "https://github.com/ressurectit/ng-datetime",
    "peerDependencies": {
        "@angular/core": ">=13.1.0",
        "@angular/common": ">=13.1.0",
        "@angular/forms": ">=13.1.0",
        "@angular/animations": ">=13.1.0",
        "rxjs": ">=6.6.7",
        "@jscrpt/common": ">=2.2.0",
        "@anglr/common": ">=10.0.0",
        "moment": "^2.29.1",
        "date-fns": "^2.27.0",
        "positions": "^1.6.2",
        "tslib": "^2.3.1"
    },
    "devDependencies": {
        "@angular/core": "^13.1.0",
        "@angular/common": "^13.1.0",
        "@angular/forms": "^13.1.0",
        "@angular/animations": "^13.1.0",
        "rxjs": "^6.6.7",
        "@jscrpt/common": "^2.2.0",
        "@anglr/common": "^10.0.0",
        "moment": "^2.29.1",
        "date-fns": "^2.27.0",
        "positions": "^1.6.2",
        "@types/jasmine": "3.6.7",
        "@angular/compiler-cli": "^13.1.0",
        "@angular/compiler": "^13.1.0",
        "@angular/language-service": "^13.1.0",
        "@ngtools/webpack": "11.2.4",
        "@microsoft/api-extractor": "7.19.2",
        "@microsoft/api-documenter": "7.13.77",
        "tslib": "2.3.1",
        "eslint": "7.32.0",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "eslint-plugin-ressurectit": "0.1.0",
        "karma": "6.2.0",
        "karma-cli": "2.0.0",
        "karma-jasmine": "4.0.1",
        "karma-chrome-launcher": "3.1.0",
        "karma-webpack": "5.0.0",
        "puppeteer": "8.0.0",
        "jasmine-core": "3.7.1",
        "webpack": "5.27.1",
        "raw-loader": "4.0.2",
        "ts-node": "9.1.1",
        "rimraf": "3.0.2",
        "copyfiles": "2.4.1",
        "replace-in-file": "6.3.2",
        "path-exists-cli": "2.0.0",
        "typescript": "4.5.4",
        "npm-git-version": "2.0.0"
    },
    "esm2020": "./es2020/src/index.js",
    "es2020": "./es2020/src/index.js",
    "es2015": "./es2015/src/index.js",
    "module": "./es2015/src/index.js",
    "typings": "./rest.d.ts",
    "exports": {
        "./package.json": {
            "default": "./package.json"
        },
        ".": {
            "types": "./rest.d.ts",
            "esm2020": "./es2020/src/index.js",
            "es2020": "./es2020/src/index.js",
            "es2015": "./es2015/src/index.js",
            "node": "./es2015/src/index.js",
            "default": "./es2020/src/index.js"
        },
        "./date-fns": {
            "types": "./date-fns/rest-date-fns.d.ts",
            "esm2020": "./es2020/date-fns/src/index.js",
            "es2020": "./es2020/date-fns/src/index.js",
            "es2015": "./es2015/date-fns/src/index.js",
            "node": "./es2015/date-fns/src/index.js",
            "default": "./es2020/date-fns/src/index.js"
        },
        "./moment": {
            "types": "./moment/rest-moment.d.ts",
            "esm2020": "./es2020/moment/src/index.js",
            "es2020": "./es2020/moment/src/index.js",
            "es2015": "./es2015/moment/src/index.js",
            "node": "./es2015/moment/src/index.js",
            "default": "./es2020/moment/src/index.js"
        }
    }
}
